from timeit import default_timer as t
from random import randint as r
import FastesSorts as sorts

count = 2000000
lst = [r(0, 1000000) for _ in range(count)]
print(lst[:10])

start = t()

#res = sorts.mergeFast(lst)

#res = sorts.mergQuick(lst)

res = sorts.merge(lst)

print(round(t() - start, 3))
print(res[:10])
